# shellcheck shell=bash

_args_readlink="readlink"
if command -v greadlink >/dev/null 2>&1; then _args_readlink="greadlink"; fi
ARGS_ROOT_DIR="$("$_args_readlink" -f "${BASH_SOURCE[0]}")"
unset -v _args_readlink

ARGS_ROOT_DIR="${ARGS_ROOT_DIR%/*}"
ARGS_ROOT_DIR="${ARGS_ROOT_DIR%/*}"

source "$ARGS_ROOT_DIR/lib/args-parse.sh"
source "$ARGS_ROOT_DIR/lib/util/util.sh"
